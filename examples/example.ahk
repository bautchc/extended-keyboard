mode := '_default'

!Space:: {
  global mode
  mode := '_default'
}

!`:: {
  global mode
  if (mode = '_menu')
    mode := 'graveAccent'
  else if (mode = '_menuPlus')
    mode := 'graveAccent_Plus'
  else
    mode := '_menu'
}

!+`:: {
  global mode
  if (mode = '_menu')
    mode := 'tilde'
  else if (mode = 'math')
    Send '≈'
  else if (mode = '_menuPlus')
    mode := 'tilde_Plus'
  else
    mode := '_menu_Plus'
}

!-:: {
  global mode
  if (mode = '_menu')
    mode := 'bar'
  else if (mode = 'katakana1')
    Send 'ー'
  else if (mode = 'katakana2')
    Send 'ー'
  else if (mode = 'katakana3')
    Send 'ー'
  else if (mode = 'math')
    Send '∓'
  else if (mode = 'superscript')
    Send '⁻'
  else if (mode = 'subscript')
    Send '₋'
  else if (mode = '_menuPlus')
    mode := 'bar_Plus'
  else
    Send '—'
}

